{
  "memory": 256,
  "ttl": 10,
  "timeout": 2,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "status": {
      "path": "/_v/status/:code",
      "public": true
    },
    "getAllTodos": {
      "path": "/_v/getAllTodos",
      "public": true
    },
    "getTodo": {
