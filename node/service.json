    },
    "createTodo": {
      "path": "/_v/createTodo",
      "public": true
    },
    "updateTodo": {
      "path": "/_v/updateTodo/:id",
      "public": true
    },
    "deleteTodo": {
      "path": "/_v/deleteTodo/:id",
      "public": true
    }
  }
